<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pol.sesion10">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Accelerometer" />
        <activity android:name=".proximity" />
        <activity android:name=".light" />
        <activity android:name=".gyroscope" />
        <activity android:name=".AmbTemp" />
        <activity android:name=".Temp" />
        <activity android:name=".orientation" />
        <activity android:name=".magnetic"></activity>
    </application>

</manifest>